{
    "compilerOptions": {
      "esModuleInterop": true,
      "skipLibCheck": true,
      "target": "es2022",
      "declaration": true,
      "allowJs": true,
      "resolveJsonModule": true,
      "moduleDetection": "force",
      "isolatedModules": true,
      "strict": true,
      "noUncheckedIndexedAccess": true,
      "module": "NodeNext"
    }
